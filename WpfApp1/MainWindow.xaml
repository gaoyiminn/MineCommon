<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="38,57,218,64" RenderTransformOrigin="0.5,0.5">
        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="-0.226"/>
                <TranslateTransform/>
            </TransformGroup>
        </Grid.RenderTransform>
        <Menu HorizontalAlignment="Left" Height="100" Margin="39,22,0,0" VerticalAlignment="Top" Width="100">
            <MenuItem Header="MenuItem" Height="100" Width="100"/>
        </Menu>
        <Button Content="Button" HorizontalAlignment="Left" Margin="227,77,0,0" VerticalAlignment="Top" Width="75" Background="{x:Null}"/>
    </Grid>
</Window>
